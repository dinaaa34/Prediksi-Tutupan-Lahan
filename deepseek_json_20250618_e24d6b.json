{
  "application": {
    "title": "Land-cover Kabupaten Indramayu",
    "style": {
      "fontSize": "22px",
      "fontWeight": "bold",
      "color": "Navy"
    }
  },
  "panel": {
    "width": "300px",
    "content": [
      {
        "type": "label",
        "value": "*Land-Cover Kabupaten Indramayu*",
        "style": {
          "fontSize": "16px",
          "fontWeight": "bold",
          "color": "Black"
        }
      },
      {
        "type": "description",
        "value": "Menggunakan Dataset citra satelit Sentinel-2 resolusi 10 m dengan feature: B2, B3, B4, B5, B6, B7, B8, B11, B12, NDVI, NDVI_STD, NDWI_STD, BSI_STD. Kelas tutupan lahan: badan air, pemukiman, sawah, kebun campuran, tambak, hutan. Total sample 1350 sampel. Proporsi train dan testing data: 60:40. Studi area: Kabupaten Indramayu and temporal tahunan dari tahun 2019 - 2023.",
        "style": {
          "fontWeight": "none",
          "fontSize": "11px",
          "margin": "10px 5px"
        }
      },
      {
        "type": "label",
        "value": "select classification method",
        "style": {
          "fontSize": "12px",
          "fontWeight": "bold",
          "color": "Black"
        }
      }
    ]
  },
  "classifier_options": [
    {
      "label": "RF",
      "value": "RF"
    },
    {
      "label": "SVM",
      "value": "SVM"
    }
  ],
  "landcover_classes": {
    "values": [0, 1, 2, 3, 4, 5],
    "names": ["badan air", "pemukiman", "sawah", "kebun campuran", "tambak", "hutan"],
    "palette": ["3399FF", "F27F7F", "F1EE11", "E2752A", "783f04", "08660A"],
    "display_names": ["Water", "Settlement", "Paddy Field", "Bare Land", "Shrimp", "Forest"]
  },
  "features": ["B2", "B3", "B4", "B5", "B6", "B7", "B8", "B11", "B12", "NDVI", "NDWI", "BSI"],
  "export_columns": ["B2", "B3", "B4", "B5", "B6", "B7", "B8", "B11", "B12", "NDVI", "NDWI", "BSI", "LULC", "sample"],
  "classification_methods": {
    "RF": {
      "parameters": {
        "trees": 900
      },
      "output_prefix": "LULC_RF_Kabupaten_Indramayu"
    },
    "SVM": {
      "parameters": {
        "kernelType": "RBF",
        "gamma": 1,
        "cost": 10
      },
      "output_prefix": "LULC_SVM_Kabupaten_Indramayu"
    }
  },
  "data_splitting": {
    "train_ratio": 0.6,
    "test_ratio": 0.4
  },
  "export_settings": {
    "scale": 10,
    "folder": {
      "RF": "LULC_RF_V3_2020",
      "SVM": "SVM_V3_2023"
    },
    "description_prefix": "LULC_V3"
  },
  "visualization": {
    "RGB_bands": ["B4", "B3", "B2"],
    "RGB_range": {
      "min": 0.0,
      "max": 0.3
    },
    "legend": {
      "position": "bottom-left",
      "padding": "8px 15px"
    },
    "charts": {
      "variable_importance": {
        "type": "ColumnChart",
        "position": "bottom-right",
        "size": {
          "width": "250px",
          "height": "250px"
        }
      },
      "area_table": {
        "type": "Table",
        "position": "top-right",
        "size": {
          "width": "200px",
          "height": "200px"
        },
        "columns": ["Land cover", "Area", "Unit"]
      }
    }
  },
  "area_calculation": {
    "unit": "Ha",
    "scale": 10
  }
}